                  <xsd:element minOccurs="0" maxOccurs="1" name="RestOfUK" type="ct:CTwholePoundStructure"/>
                  <xsd:element minOccurs="1" maxOccurs="1" name="Total" type="ct:CTwholePoundStructure"/>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" name="GroupReliefForCarriedForwardLosses">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element minOccurs="0" maxOccurs="1" name="NorthernIreland" type="ct:CTwholePoundStructure"/>
                  <xsd:element minOccurs="0" maxOccurs="1" name="RestOfUK" type="ct:CTwholePoundStructure"/>
                  <xsd:element minOccurs="1" maxOccurs="1" name="Total" type="ct:CTwholePoundStructure"/>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" name="ChargeableProfits">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element minOccurs="0" maxOccurs="1" name="NorthernIreland" type="ct:CTwholePoundStructure"/>
                  <xsd:element minOccurs="0" maxOccurs="1" name="RestOfUK" type="ct:CTwholePoundStructure"/>
                  <xsd:element minOccurs="1" maxOccurs="1" name="Total" type="ct:CTwholePoundStructure"/>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="1" name="TaxReconciliation">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" maxOccurs="1" name="ResearchAndDevelopmentCredit">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element minOccurs="0" maxOccurs="1" name="NorthernIreland" type="ct:CTpoundPenceStructure"/>
                  <xsd:element minOccurs="0" maxOccurs="1" name="RestOfUK" type="ct:CTpoundPenceStructure"/>
                  <xsd:element minOccurs="1" maxOccurs="1" name="Total" type="ct:CTpoundPenceStructure"/>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" name="CreativeCredit">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element minOccurs="0" maxOccurs="1" name="NorthernIreland" type="ct:CTpoundPenceStructure"/>
                  <xsd:element minOccurs="0" maxOccurs="1" name="RestOfUK" type="ct:CTpoundPenceStructure"/>
                  <xsd:element minOccurs="1" maxOccurs="1" name="Total" type="ct:CTpoundPenceStructure"/>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" name="ResearchAndDevelopmentOrCreativeTaxCredit">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element minOccurs="0" maxOccurs="1" name="NorthernIreland" type="ct:CTpoundPenceStructure"/>
                  <xsd:element minOccurs="0" maxOccurs="1" name="RestOfUK" type="ct:CTpoundPenceStructure"/>
                  <xsd:element minOccurs="1" maxOccurs="1" name="Total" type="ct:CTpoundPenceStructure"/>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" name="LandRemediationCredit">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element minOccurs="0" maxOccurs="1" name="NorthernIreland" type="ct:CTpoundPenceStructure"/>
                  <xsd:element minOccurs="0" maxOccurs="1" name="RestOfUK" type="ct:CTpoundPenceStructure"/>
                  <xsd:element minOccurs="1" maxOccurs="1" name="Total" type="ct:CTpoundPenceStructure"/>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" name="LifeAssuranceCompanyCredit">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element minOccurs="1" maxOccurs="1" name="RestOfUK" type="ct:CTpoundPenceStructure"/>
                  <xsd:element minOccurs="1" maxOccurs="1" name="Total" type="ct:CTpoundPenceStructure"/>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" name="TotalLandAndLifeCredit">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element minOccurs="0" maxOccurs="1" name="NorthernIreland" type="ct:CTpoundPenceStructure"/>
                  <xsd:element minOccurs="0" maxOccurs="1" name="RestOfUK" type="ct:CTpoundPenceStructure"/>
                  <xsd:element minOccurs="1" maxOccurs="1" name="Total" type="ct:CTpoundPenceStructure"/>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" name="CapitalAllowancesFirstYearCredit">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element minOccurs="0" maxOccurs="1" name="NorthernIreland" type="ct:CTpoundPenceStructure"/>
                  <xsd:element minOccurs="0" maxOccurs="1" name="RestOfUK" type="ct:CTpoundPenceStructure"/>
                  <xsd:element minOccurs="1" maxOccurs="1" name="Total" type="ct:CTpoundPenceStructure"/>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="1" name="LossesDeficitsAndExcess">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" maxOccurs="1" name="LossesOfTradesUK">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element minOccurs="0" maxOccurs="1" name="NorthernIreland" type="ct:CTwholePoundStructure"/>
                  <xsd:element minOccurs="0" maxOccurs="1" name="RestOfUK" type="ct:CTwholePoundStructure"/>
                  <xsd:element minOccurs="1" maxOccurs="1" name="Total" type="ct:CTwholePoundStructure"/>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" name="LossesOfTradesOutsideUK">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element minOccurs="1" maxOccurs="1" name="RestOfUK" type="ct:CTwholePoundStructure"/>
                  <xsd:element minOccurs="1" maxOccurs="1" name="Total" type="ct:CTwholePoundStructure"/>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="1" name="Indicators">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" maxOccurs="1" name="BackOfficeActivities" type="ct:CT_YesType"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="PreCommencementAssets" type="ct:CT_YesType"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="SMEelectionIntoLargeCompanyRegime" type="ct:CT_YesType"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="CTH">
    <xsd:sequence>
      <xsd:element minOccurs="1" maxOccurs="unbounded" name="Royalties">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="1" maxOccurs="1" name="RecipientName">
              <xsd:simpleType>
                <xsd:restriction base="ct:CTexcludedCharsStringType">
                  <xsd:minLength value="2"/>
                  <xsd:maxLength value="56"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="1" maxOccurs="1" name="AddressOfRecipient" type="ct:CTaddressStructure"/>
            <xsd:element minOccurs="1" maxOccurs="1" name="PaymentType">
              <xsd:simpleType>
                <xsd:restriction base="ct:CTexcludedCharsStringType">
                  <xsd:minLength value="2"/>
                  <xsd:maxLength value="56"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="1" maxOccurs="1" name="Amount" type="ct:CT_CTnonZeroWholePoundStructure"/>
            <xsd:element minOccurs="1" maxOccurs="1" name="RoyaltiesAgreement">
              <xsd:complexType>
                <xsd:choice minOccurs="1" maxOccurs="1">
                  <xsd:element minOccurs="1" maxOccurs="1" name="InterestAndRoyalties">
                    <xsd:simpleType>
                      <xsd:restriction base="ct:CTexcludedCharsStringType">
                        <xsd:minLength value="2"/>
                        <xsd:maxLength value="56"/>
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                  <xsd:element minOccurs="1" maxOccurs="1" name="DoubleTaxationAgreement">
                    <xsd:simpleType>
                      <xsd:restriction base="ct:CTexcludedCharsStringType">
                        <xsd:minLength value="2"/>
                        <xsd:maxLength value="56"/>
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                </xsd:choice>
              </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="1" maxOccurs="1" name="DeductionRate">
              <xsd:simpleType>
                <xsd:restriction base="ct:CT_IRnonNegativeDecimalType">
                  <xsd:maxInclusive value="100.00"/>
                  <xsd:minInclusive value="0.00"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="1" maxOccurs="1" name="DeductionAmount">
              <xsd:complexType>
                <xsd:simpleContent>
                  <xsd:restriction base="ct:CTpoundPenceStructure">
                    <xsd:maxInclusive value="99999999999.99"/>
                  </xsd:restriction>
                </xsd:simpleContent>
              </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" name="AdditionalNotes">
              <xsd:simpleType>
                <xsd:restriction base="ct:CTexcludedCharsStringType">
                  <xsd:minLength value="2"/>
                  <xsd:maxLength value="56"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="CTI">
    <xsd:sequence>
      <xsd:element minOccurs="1" maxOccurs="1" name="CalculationOfSupplementaryCharge">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="1" maxOccurs="1" name="Trade">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element minOccurs="1" maxOccurs="1" name="Amount" type="ct:CTwholePoundStructure"/>
                  <xsd:choice minOccurs="1" maxOccurs="1">
                    <xsd:element minOccurs="1" maxOccurs="1" name="Profits" type="ct:CT_YesType"/>
                    <xsd:element minOccurs="1" maxOccurs="1" name="Losses" type="ct:CT_YesType"/>
                  </xsd:choice>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" name="DisallowedFinancingCosts">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element minOccurs="0" maxOccurs="1" name="RelatedToCompany" type="ct:CT_CTnonZeroWholePoundStructure"/>
                  <xsd:element minOccurs="0" maxOccurs="1" name="RelatedToLossRelief" type="ct:CT_CTnonZeroWholePoundStructure"/>
                  <xsd:element minOccurs="1" maxOccurs="1" name="Total" type="ct:CTwholePoundStructure"/>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="1" maxOccurs="1" name="Profits" type="ct:CTwholePoundStructure"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="DecommissioningAdjustment" type="ct:CTwholePoundStructure"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="RevisedProfits" type="ct:CTwholePoundStructure"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="MinusLosses" type="ct:CT_CTnonZeroWholePoundStructure"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="DecommissioningReduction" type="ct:CTwholePoundStructure"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="FieldAllowance" type="ct:CTwholePoundStructure"/>
            <xsd:element minOccurs="1" maxOccurs="1" name="NetProfits" type="ct:CTwholePoundStructure"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="Tax" type="ct:CT_CTnonZeroPoundPenceStructure"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="1" name="LossesArising" type="ct:CT_CTnonZeroWholePoundStructure"/>
      <xsd:element minOccurs="0" maxOccurs="1" name="NetRingFenceTrade">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" maxOccurs="1" name="RingFenceCorpTaxIncluded" type="ct:CTpoundPenceStructure"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="SupplementaryChargeTax" type="ct:CTpoundPenceStructure"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="1" name="TransferredTaxHistory">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="1" maxOccurs="1" name="ElectionToTransfer">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element minOccurs="0" maxOccurs="1" name="AssetAcquiredThisPeriod" type="ct:CT_YesType"/>
                  <xsd:element minOccurs="0" maxOccurs="1" name="AssetDisposedOfThisPeriod" type="ct:CT_YesType"/>
                  <xsd:element minOccurs="0" maxOccurs="1" name="AssetAcquiredPreviousPeriod" type="ct:CT_YesType"/>
                  <xsd:element minOccurs="0" maxOccurs="1" name="AssetDisposedOfPreviousPeriod" type="ct:CT_YesType"/>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="999" name="AssetInformation">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element minOccurs="1" maxOccurs="1" name="DescriptionOfAsset">
                    <xsd:simpleType>
                      <xsd:restriction base="ct:CT_CTstringType">
                        <xsd:maxLength value="300"/>
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                  <xsd:element minOccurs="1" maxOccurs="1" name="AssetReference">
                    <xsd:simpleType>
                      <xsd:restriction base="ct:CT_CTstringType">
                        <xsd:pattern value="[A-Z]{6}[0-9]{3}"/>
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                  <xsd:element minOccurs="0" maxOccurs="1" name="OGAapprovedCessation" type="ct:CT_YesType"/>
                  <xsd:element minOccurs="0" maxOccurs="1" name="STOcertificationQualified" type="ct:CT_YesType"/>
                  <xsd:element minOccurs="1" maxOccurs="1" name="TrackingOfTransferredTaxHistory">
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element minOccurs="0" maxOccurs="1" name="IncludedInTaxComputations" type="ct:CT_YesType"/>
                        <xsd:element minOccurs="1" maxOccurs="1" name="RingFenceCorporationTax">
                          <xsd:complexType>
                            <xsd:sequence>
                              <xsd:element minOccurs="1" maxOccurs="1" name="AmountBroughtForward" type="ct:CT_ProfitsTaxStructure"/>
                              <xsd:element minOccurs="0" maxOccurs="1" name="AmountTransferred" type="ct:CT_ProfitsTaxStructure"/>
                              <xsd:element minOccurs="0" maxOccurs="1" name="AmountUsed" type="ct:CT_ProfitsTaxStructure"/>
                              <xsd:element minOccurs="1" maxOccurs="1" name="AmountCarriedForward" type="ct:CT_ProfitsTaxStructure"/>
                            </xsd:sequence>
                          </xsd:complexType>
                        </xsd:element>
                        <xsd:element minOccurs="0" maxOccurs="1" name="SupplementaryCharge">
                          <xsd:complexType>
                            <xsd:sequence>
                              <xsd:element minOccurs="1" maxOccurs="1" name="AmountBroughtForward" type="ct:CT_ProfitsTaxStructure"/>
                              <xsd:element minOccurs="0" maxOccurs="1" name="AmountTransferred" type="ct:CT_ProfitsTaxStructure"/>
                              <xsd:element minOccurs="0" maxOccurs="1" name="AmountUsed" type="ct:CT_ProfitsTaxStructure"/>
                              <xsd:element minOccurs="1" maxOccurs="1" name="AmountCarriedForward" type="ct:CT_ProfitsTaxStructure"/>
                            </xsd:sequence>
                          </xsd:complexType>
                        </xsd:element>
                        <xsd:element minOccurs="1" maxOccurs="1" name="TrackedProfitsOrLosses">
                          <xsd:complexType>
                            <xsd:sequence>
                              <xsd:element minOccurs="0" maxOccurs="1" name="BalanceBroughtForward" type="ct:CT_ProfitsLossesStructure"/>
                              <xsd:element minOccurs="1" maxOccurs="1" name="ProfitsOrLossesForThisPeriod" type="ct:CT_ProfitsLossesStructure"/>
                              <xsd:choice minOccurs="0" maxOccurs="1">
                                <xsd:element minOccurs="1" maxOccurs="1" name="OtherAdjustmentsPlus" type="ct:CTwholePoundStructure"/>
                                <xsd:element minOccurs="1" maxOccurs="1" name="OtherAdjustmentsMinus" type="ct:CTwholePoundStructure"/>
                              </xsd:choice>
                              <xsd:element minOccurs="1" maxOccurs="1" name="AmountCarriedForward" type="ct:CT_ProfitsLossesStructure"/>
                            </xsd:sequence>
                          </xsd:complexType>
                        </xsd:element>
                        <xsd:element minOccurs="0" maxOccurs="1" name="DecommissioningExpenditure">
                          <xsd:complexType>
                            <xsd:sequence>
                              <xsd:element minOccurs="0" maxOccurs="1" name="BalanceBroughtForward" type="ct:CTwholePoundStructure"/>
                              <xsd:element minOccurs="0" maxOccurs="1" name="ExpenditureForThisPeriod" type="ct:CTwholePoundStructure"/>
                              <xsd:choice minOccurs="0" maxOccurs="1">
                                <xsd:element minOccurs="1" maxOccurs="1" name="OtherAdjustmentsPlus" type="ct:CTwholePoundStructure"/>
                                <xsd:element minOccurs="1" maxOccurs="1" name="OtherAdjustmentsMinus" type="ct:CTwholePoundStructure"/>
                              </xsd:choice>
                              <xsd:element minOccurs="1" maxOccurs="1" name="AmountCarriedForward" type="ct:CTwholePoundStructure"/>
                            </xsd:sequence>
                          </xsd:complexType>
                        </xsd:element>
                        <xsd:element minOccurs="0" maxOccurs="1" name="ActivatedRingFenceCorporationTax">
                          <xsd:complexType>
                            <xsd:sequence>
                              <xsd:element minOccurs="0" maxOccurs="1" name="PreviouslyActivated" type="ct:CT_ProfitsTaxStructure"/>
                              <xsd:element minOccurs="0" maxOccurs="1" name="ActivatedThisPeriod" type="ct:CT_ProfitsTaxStructure"/>
                              <xsd:element minOccurs="1" maxOccurs="1" name="TotalActivated" type="ct:CT_ProfitsTaxStructure"/>
                              <xsd:element minOccurs="0" maxOccurs="1" name="TotalUsed" type="ct:CT_ProfitsTaxStructure"/>
                            </xsd:sequence>
                          </xsd:complexType>
                        </xsd:element>
                        <xsd:element minOccurs="0" maxOccurs="1" name="ActivatedSupplementaryCharge">
                          <xsd:complexType>
                            <xsd:sequence>
                              <xsd:element minOccurs="0" maxOccurs="1" name="PreviouslyActivated" type="ct:CT_ProfitsTaxStructure"/>
                              <xsd:element minOccurs="0" maxOccurs="1" name="ActivatedThisPeriod" type="ct:CT_ProfitsTaxStructure"/>
                              <xsd:element minOccurs="1" maxOccurs="1" name="TotalActivated" type="ct:CT_ProfitsTaxStructure"/>
                              <xsd:element minOccurs="0" maxOccurs="1" name="TotalUsed" type="ct:CT_ProfitsTaxStructure"/>
                            </xsd:sequence>
                          </xsd:complexType>
                        </xsd:element>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="CTJ">
    <xsd:sequence>
      <xsd:element minOccurs="1" maxOccurs="10" name="AvoidanceSchemes">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="1" maxOccurs="1" name="ReferenceNumber">
              <xsd:simpleType>
                <xsd:restriction base="xsd:integer">
                  <xsd:pattern value="[0-9]{8}"/>
                  <xsd:maxInclusive value="99999999"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="1" maxOccurs="1" name="AccountingPeriod" type="ct:CT_DateType"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="CTK">
    <xsd:sequence>
      <xsd:element minOccurs="1" maxOccurs="1" name="TaxCalculation">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="1" maxOccurs="1" name="SAbeforeRestitutionTax" type="ct:CTpoundPenceStructure"/>
            <xsd:element minOccurs="1" maxOccurs="1" name="RestitutionInterest" type="ct:CTwholePoundStructure"/>
            <xsd:element minOccurs="1" maxOccurs="1" name="TaxCalculation">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element minOccurs="1" maxOccurs="1" name="FinancialYearOne">
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element minOccurs="1" maxOccurs="1" name="Year">
                          <xsd:simpleType>
                            <xsd:restriction base="xsd:gYear">
                              <xsd:pattern value="[0-9]{4}"/>
                            </xsd:restriction>
                          </xsd:simpleType>
                        </xsd:element>
                        <xsd:element minOccurs="1" maxOccurs="1" name="AmountOfInterest" type="ct:CTwholePoundStructure"/>
                        <xsd:element minOccurs="1" maxOccurs="1" name="TaxRate">
                          <xsd:simpleType>
                            <xsd:restriction base="ct:CT_IRnonNegativeDecimalType">
                              <xsd:maxInclusive value="100.00"/>
                              <xsd:minInclusive value="0.00"/>
                            </xsd:restriction>
                          </xsd:simpleType>
                        </xsd:element>
                        <xsd:element minOccurs="1" maxOccurs="1" name="Tax" type="ct:CTpoundPenceStructure"/>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element minOccurs="0" maxOccurs="1" name="FinancialYearTwo">
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element minOccurs="1" maxOccurs="1" name="Year">
                          <xsd:simpleType>
                            <xsd:restriction base="xsd:gYear">
                              <xsd:pattern value="[0-9]{4}"/>
                            </xsd:restriction>
                          </xsd:simpleType>
                        </xsd:element>
                        <xsd:element minOccurs="1" maxOccurs="1" name="AmountOfInterest" type="ct:CTwholePoundStructure"/>
                        <xsd:element minOccurs="1" maxOccurs="1" name="TaxRate">
                          <xsd:simpleType>
                            <xsd:restriction base="ct:CT_IRnonNegativeDecimalType">
                              <xsd:maxInclusive value="100.00"/>
                              <xsd:minInclusive value="0.00"/>
                            </xsd:restriction>
                          </xsd:simpleType>
                        </xsd:element>
                        <xsd:element minOccurs="1" maxOccurs="1" name="Tax" type="ct:CTpoundPenceStructure"/>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="1" maxOccurs="1" name="TotalRestitutionTax" type="ct:CTpoundPenceStructure"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="TaxAlreadyWithheld" type="ct:CTpoundPenceStructure"/>
            <xsd:element minOccurs="1" maxOccurs="1" name="SAafterRestitutionTax" type="ct:CTpoundPenceStructure"/>
            <xsd:element minOccurs="1" maxOccurs="1" name="RestitutionTaxNowPayable" type="ct:CTpoundPenceStructure"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="YesNoType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="no"/>
      <xsd:enumeration value="yes"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="CTnominalShareValueStructure">
    <xsd:simpleContent>
      <xsd:extension base="ct:_intermediate_CTnominalShareValueStructure">
        <xsd:attribute use="optional" name="Currency" type="ct:CT_ISOcurrencyType"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:simpleType name="CT_CTaddressLineType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[A-Za-z0-9 ,\.\(\)/&amp;&apos;\-&quot;!%\*_\+:@&lt;&gt;\?=;]*"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="CT_CTnonZeroPoundPenceStructure">
    <xsd:simpleContent>
      <xsd:restriction base="ct:CT_IRmonetaryStructure">
        <xsd:minExclusive value="0.00"/>
      </xsd:restriction>
    </xsd:simpleContent>
  </xsd:complexType>
