  <xsd:element name="IRenvelope">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element minOccurs="1" maxOccurs="1" ref="ct:IRheader"/>
        <xsd:element minOccurs="1" maxOccurs="1" name="CompanyTaxReturn">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element minOccurs="1" maxOccurs="1" name="CompanyInformation">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element minOccurs="1" maxOccurs="1" name="CompanyName">
                      <xsd:simpleType>
                        <xsd:restriction base="ct:CTexcludedCharsStringType">
                          <xsd:minLength value="2"/>
                          <xsd:maxLength value="56"/>
                        </xsd:restriction>
                      </xsd:simpleType>
                    </xsd:element>
                    <xsd:element minOccurs="0" maxOccurs="1" name="RegistrationNumber">
                      <xsd:simpleType>
                        <xsd:restriction base="ct:CT_CTstringType">
                          <xsd:minLength value="2"/>
                          <xsd:maxLength value="8"/>
                          <xsd:pattern value="[A-Z0-9]{2,8}"/>
                        </xsd:restriction>
                      </xsd:simpleType>
                    </xsd:element>
                    <xsd:element minOccurs="1" maxOccurs="1" name="Reference" type="ct:CT_UTRtype"/>
                    <xsd:element minOccurs="1" maxOccurs="1" name="CompanyType">
                      <xsd:simpleType>
                        <xsd:restriction base="xsd:integer">
                          <xsd:maxInclusive value="10"/>
                          <xsd:minInclusive value="0"/>
                        </xsd:restriction>
                      </xsd:simpleType>
                    </xsd:element>
                    <xsd:element minOccurs="0" maxOccurs="1" name="NorthernIreland">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element minOccurs="1" maxOccurs="1" name="NItradingActivity" type="ct:CT_YesType"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="SME" type="ct:CT_YesType"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="NIemployer" type="ct:CT_YesType"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="SpecialCircumstances" type="ct:CT_YesType"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element minOccurs="1" maxOccurs="1" name="PeriodCovered">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element minOccurs="1" maxOccurs="1" name="From">
                            <xsd:simpleType>
                              <xsd:restriction base="ct:CT_DateType">
                                <xsd:minInclusive value="2015-04-01"/>
                              </xsd:restriction>
                            </xsd:simpleType>
                          </xsd:element>
                          <xsd:element minOccurs="1" maxOccurs="1" name="To" type="ct:CT_DateType"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element minOccurs="1" maxOccurs="1" name="ReturnInfoSummary">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="1" name="ThisPeriod" type="ct:CT_YesType"/>
                    <xsd:element minOccurs="0" maxOccurs="1" name="EarlierPeriod" type="ct:CT_YesType"/>
                    <xsd:element minOccurs="0" maxOccurs="1" name="MultipleReturns" type="ct:CT_YesType"/>
                    <xsd:element minOccurs="0" maxOccurs="1" name="ProvisionalFigures" type="ct:CT_YesType"/>
                    <xsd:element minOccurs="0" maxOccurs="1" name="PartOfNonSmallGroup" type="ct:CT_YesType"/>
                    <xsd:element minOccurs="0" maxOccurs="1" name="RegisteredAvoidanceScheme" type="ct:CT_YesType"/>
                    <xsd:element minOccurs="0" maxOccurs="1" name="TransferPricing">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element minOccurs="0" maxOccurs="1" name="Adjustment" type="ct:CT_YesType"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="SME" type="ct:CT_YesType"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element minOccurs="1" maxOccurs="1" name="Accounts">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element minOccurs="0" maxOccurs="1" name="ThisPeriodAccounts" type="ct:CT_YesType"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="DifferentPeriod" type="ct:CT_YesType"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="NoAccountsReason">
                            <xsd:simpleType>
                              <xsd:restriction base="ct:CTexcludedCharsStringType">
                                <xsd:minLength value="2"/>
                                <xsd:maxLength value="40"/>
                                <xsd:enumeration value="PoA differs from AP-a/cs with sep rtn">
                                  <xsd:annotation>
                                    <xsd:documentation>Submitted with accompanying return</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:enumeration>
                                <xsd:enumeration value="Company in liquidation">
                                  <xsd:annotation>
                                    <xsd:documentation>Company in liquidation</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:enumeration>
                                <xsd:enumeration value="Not within charge to CT">
                                  <xsd:annotation>
                                    <xsd:documentation>Not within charge to CT</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:enumeration>
                                <xsd:enumeration value="Company dormant">
                                  <xsd:annotation>
                                    <xsd:documentation>Company dormant</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:enumeration>
                                <xsd:enumeration value="Amendment - a/cs already submitted">
                                  <xsd:annotation>
                                    <xsd:documentation>Amendment - a/cs already submitted</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:enumeration>
                                <xsd:enumeration value="Other - PDF attached with explanation">
                                  <xsd:annotation>
                                    <xsd:documentation>Other - PDF attached with explanation</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:enumeration>
                                <xsd:enumeration value="PDF accounts attached with explanation">
                                  <xsd:annotation>
                                    <xsd:documentation>PDF accounts attached with explanation</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:enumeration>
                              </xsd:restriction>
                            </xsd:simpleType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element minOccurs="1" maxOccurs="1" name="Computations">
                      <xsd:complexType>
                        <xsd:choice>
                          <xsd:element minOccurs="1" maxOccurs="1" name="ThisPeriodComputations" type="ct:CT_YesType"/>
                          <xsd:element minOccurs="1" maxOccurs="1" name="DifferentPeriod" type="ct:CT_YesType"/>
                          <xsd:element minOccurs="1" maxOccurs="1" name="NoComputationsReason">
                            <xsd:simpleType>
                              <xsd:restriction base="ct:CTexcludedCharsStringType">
                                <xsd:minLength value="2"/>
                                <xsd:maxLength value="40"/>
                                <xsd:enumeration value="PoA differs from AP-comp with sep rtn">
                                  <xsd:annotation>
                                    <xsd:documentation>Long period of account - computations submitted with accompanying return</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:enumeration>
                                <xsd:enumeration value="Company in liquidation">
                                  <xsd:annotation>
                                    <xsd:documentation>Company in liquidation</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:enumeration>
                                <xsd:enumeration value="Not within charge to CT">
                                  <xsd:annotation>
                                    <xsd:documentation>Not within charge to CT</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:enumeration>
                                <xsd:enumeration value="Company dormant">
                                  <xsd:annotation>
                                    <xsd:documentation>Company dormant</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:enumeration>
                                <xsd:enumeration value="Amendment - comps already submitted">
                                  <xsd:annotation>
                                    <xsd:documentation>Amendment - comps already submitted</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:enumeration>
                                <xsd:enumeration value="Other - PDF attached with explanation">
                                  <xsd:annotation>
                                    <xsd:documentation>Other - PDF attached with explanation</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:enumeration>
                              </xsd:restriction>
                            </xsd:simpleType>
                          </xsd:element>
                        </xsd:choice>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element minOccurs="0" maxOccurs="1" name="SupplementaryPages">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element minOccurs="0" maxOccurs="1" name="CT600A" type="ct:CT_YesType"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="CT600B" type="ct:CT_YesType"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="CT600C" type="ct:CT_YesType"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="CT600D" type="ct:CT_YesType"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="CT600E" type="ct:CT_YesType"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="CT600F" type="ct:CT_YesType"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="CT600G" type="ct:CT_YesType"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="CT600H" type="ct:CT_YesType"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="CT600I" type="ct:CT_YesType"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="CT600J" type="ct:CT_YesType"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="CT600K" type="ct:CT_YesType"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element minOccurs="0" maxOccurs="1" name="Turnover">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="1" name="Total">
                      <xsd:complexType>
                        <xsd:simpleContent>
                          <xsd:restriction base="ct:IRnonNegativeWholeUnitsMonetaryStructure">
                            <xsd:maxInclusive value="999999999999999999.00"/>
                          </xsd:restriction>
                        </xsd:simpleContent>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element minOccurs="0" maxOccurs="1" name="OtherFinancialConcerns" type="ct:CT_YesType"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element minOccurs="1" maxOccurs="1" name="CompanyTaxCalculation">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element minOccurs="1" maxOccurs="1" name="Income">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element minOccurs="0" maxOccurs="1" name="Trading">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element minOccurs="0" maxOccurs="1" name="Profits" type="ct:CTwholePoundStructure"/>
                                <xsd:element minOccurs="0" maxOccurs="1" name="LossesBroughtForward" type="ct:CTwholePoundStructure"/>
                                <xsd:element minOccurs="0" maxOccurs="1" name="NetProfits" type="ct:CTwholePoundStructure"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element minOccurs="0" maxOccurs="1" name="NonTradingLoanProfitsAndGains" type="ct:CTwholePoundStructure"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="IncomeStatedNet" type="ct:CT_YesType"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="NonLoanAnnuitiesAnnualPaymentsDiscounts" type="ct:CTwholePoundStructure"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="NonUKdividends" type="ct:CTwholePoundStructure"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="DeductedIncome" type="ct:CTwholePoundStructure"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="PropertyBusinessIncome" type="ct:CTwholePoundStructure"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="NonTradingGainsIntangibles" type="ct:CTwholePoundStructure"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="TonnageTaxProfits" type="ct:CTwholePoundStructure"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="OtherIncome" type="ct:CTwholePoundStructure"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element minOccurs="0" maxOccurs="1" name="ChargeableGains">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element minOccurs="0" maxOccurs="1" name="GrossGains" type="ct:CTwholePoundStructure"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="AllowableLosses" type="ct:CTwholePoundStructure"/>
                          <xsd:element minOccurs="1" maxOccurs="1" name="NetChargeableGains" type="ct:CTwholePoundStructure"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element minOccurs="0" maxOccurs="1" name="LossesBroughtForward" type="ct:CTwholePoundStructure"/>
                    <xsd:element minOccurs="0" maxOccurs="1" name="NonTradeDeficitsOnLoans" type="ct:CTwholePoundStructure"/>
                    <xsd:element minOccurs="0" maxOccurs="1" name="ProfitsBeforeOtherDeductions" type="ct:CTwholePoundStructure"/>
                    <xsd:element minOccurs="0" maxOccurs="1" name="DeductionsAndReliefs">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element minOccurs="0" maxOccurs="1" name="UnquotedShares" type="ct:CTwholePoundStructure"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="ManagementExpenses" type="ct:CTwholePoundStructure"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="UKpropertyBusinessLosses" type="ct:CTwholePoundStructure"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="CapitalAllowances" type="ct:CTwholePoundStructure"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="NonTradeDeficits" type="ct:CTwholePoundStructure"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="CarriedForwardNonTradeDeficits" type="ct:CTwholePoundStructure"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="NonTradingLossIntangibles" type="ct:CTwholePoundStructure"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="TradingLosses" type="ct:CTwholePoundStructure"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="TradingLossesCarriedBack" type="ct:CT_YesType"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="TradingLossesCarriedForward" type="ct:CTwholePoundStructure"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="NonTradeCapitalAllowances" type="ct:CTwholePoundStructure"/>
                          <xsd:element minOccurs="0" maxOccurs="1" name="Total" type="ct:CTwholePoundStructure"/>
                        </xsd:sequence>
                      </xsd:complexType>
