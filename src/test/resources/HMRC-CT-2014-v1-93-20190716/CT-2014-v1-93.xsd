              <xsd:element minOccurs="0" maxOccurs="1" name="GroupAndConsortium" type="ct:CTC"/>
              <xsd:element minOccurs="0" maxOccurs="1" name="Insurance" type="ct:CTD"/>
              <xsd:element minOccurs="0" maxOccurs="1" name="Charity" type="ct:CTE"/>
              <xsd:element minOccurs="0" maxOccurs="1" name="TonnageTax" type="ct:CTF"/>
              <xsd:element minOccurs="0" maxOccurs="1" name="NorthernIreland" type="ct:CTG"/>
              <xsd:element minOccurs="0" maxOccurs="1" name="CrossBorderRoyalties" type="ct:CTH"/>
              <xsd:element minOccurs="0" maxOccurs="1" name="RingFenceTrade" type="ct:CTI"/>
              <xsd:element minOccurs="0" maxOccurs="1" name="TaxAvoidanceSchemes" type="ct:CTJ"/>
              <xsd:element minOccurs="0" maxOccurs="1" name="RestitutionTax" type="ct:CTK"/>
              <xsd:element minOccurs="0" maxOccurs="1" name="WelshReturn" type="ct:CT_YesType"/>
              <xsd:element minOccurs="0" maxOccurs="1" name="JointAccounts" type="ct:CT_YesType"/>
              <xsd:element minOccurs="0" maxOccurs="1" name="AttachedFiles">
                <xsd:complexType>
                  <xsd:choice minOccurs="1" maxOccurs="1">
                    <xsd:element minOccurs="1" maxOccurs="unbounded" name="Attachment" type="ct:AttachmentStructure"/>
                    <xsd:sequence>
                      <xsd:element minOccurs="1" maxOccurs="1" name="XBRLsubmission" type="ct:XBRLsubmission"/>
                      <xsd:element minOccurs="0" maxOccurs="unbounded" name="Attachment" type="ct:AttachmentStructure"/>
                    </xsd:sequence>
                  </xsd:choice>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
            <xsd:attribute use="required" name="ReturnType">
              <xsd:simpleType>
                <xsd:restriction base="ct:CT_CTstringType">
                  <xsd:enumeration value="amended">
                    <xsd:annotation>
                      <xsd:documentation>this is an amended return</xsd:documentation>
                    </xsd:annotation>
                  </xsd:enumeration>
                  <xsd:enumeration value="new">
                    <xsd:annotation>
                      <xsd:documentation>this is a new return</xsd:documentation>
                    </xsd:annotation>
                  </xsd:enumeration>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:attribute>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="IRheader">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element minOccurs="0" maxOccurs="1" name="Keys">
          <xsd:annotation>
            <xsd:documentation>This element contains identifiers for the sender of the message. Each identifier is contained within a &lt;Key&gt; element. Each &lt;Key&gt; element also contains a 'Type' attribute which describes the identifier type (e.g. &lt;Key Type='UTR'&gt;, &lt;Key Type='TaxOfficeNumber'&gt;, &lt;Key Type='TaxOfficeReference'&gt;).</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element minOccurs="1" maxOccurs="unbounded" name="Key">
                <xsd:complexType>
                  <xsd:simpleContent>
                    <xsd:extension base="ct:CT_IRheaderStringType">
                      <xsd:attribute use="optional" name="Type" type="ct:CT_IRheaderStringType"/>
                    </xsd:extension>
                  </xsd:simpleContent>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element minOccurs="1" maxOccurs="1" name="PeriodEnd" type="ct:CT_DateType"/>
        <xsd:element minOccurs="0" maxOccurs="1" name="Principal">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element minOccurs="1" maxOccurs="1" name="Contact" type="ct:CT_ContactDetailsStructure"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element minOccurs="0" maxOccurs="1" name="Agent">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element minOccurs="0" maxOccurs="1" name="AgentID">
                <xsd:annotation>
                  <xsd:documentation>This identifier is for the agent's own reference and is not the same as any agent's credentials that might be used to identify the agent to the Government Gateway.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                  <xsd:restriction base="ct:CT_IRheaderStringType">
                    <xsd:minLength value="1"/>
                    <xsd:maxLength value="35"/>
                  </xsd:restriction>
                </xsd:simpleType>
              </xsd:element>
              <xsd:element minOccurs="0" maxOccurs="1" name="Company">
                <xsd:simpleType>
                  <xsd:restriction base="ct:CT_IRheaderStringType">
                    <xsd:minLength value="1"/>
                    <xsd:maxLength value="35"/>
                  </xsd:restriction>
                </xsd:simpleType>
              </xsd:element>
              <xsd:element minOccurs="0" maxOccurs="1" name="Address">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element minOccurs="1" maxOccurs="4" name="Line">
                      <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                          <xsd:minLength value="1"/>
                          <xsd:maxLength value="35"/>
                          <xsd:pattern value="[A-Za-z0-9 ~!&quot;@#$%&amp;&apos;\(\)\*\+,\-\./:;&lt;=&gt;\?\[\\\]^_\{\}£€]*"/>
                        </xsd:restriction>
                      </xsd:simpleType>
                    </xsd:element>
                    <xsd:element minOccurs="0" maxOccurs="1" name="PostCode">
                      <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                          <xsd:minLength value="1"/>
                          <xsd:maxLength value="8"/>
                          <xsd:pattern value="[A-Za-z0-9 ~!&quot;@#$%&amp;&apos;\(\)\*\+,\-\./:;&lt;=&gt;\?\[\\\]^_\{\}£€]*"/>
                        </xsd:restriction>
                      </xsd:simpleType>
                    </xsd:element>
                    <xsd:element minOccurs="0" maxOccurs="1" name="Country">
                      <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                          <xsd:minLength value="1"/>
                          <xsd:maxLength value="35"/>
                          <xsd:pattern value="[A-Za-z0-9 ~!&quot;@#$%&amp;&apos;\(\)\*\+,\-\./:;&lt;=&gt;\?\[\\\]^_\{\}£€]*"/>
                        </xsd:restriction>
                      </xsd:simpleType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element minOccurs="0" maxOccurs="1" name="Contact" type="ct:CT_ContactDetailsStructure"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element minOccurs="0" maxOccurs="1" name="DefaultCurrency">
          <xsd:simpleType>
            <xsd:restriction base="ct:CT_ISOcurrencyType">
              <xsd:enumeration value="GBP">
                <xsd:annotation>
                  <xsd:documentation>GBP</xsd:documentation>
                </xsd:annotation>
              </xsd:enumeration>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
        <xsd:element minOccurs="0" maxOccurs="1" name="Manifest">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element ref="ct:Contains"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element minOccurs="0" maxOccurs="1" name="IRmark">
          <xsd:complexType>
            <xsd:simpleContent>
              <xsd:extension base="xsd:string">
                <xsd:attribute use="required" name="Type">
                  <xsd:simpleType>
                    <xsd:restriction base="ct:CT_IRheaderStringType">
                      <xsd:enumeration value="SAonly">
                        <xsd:annotation>
                          <xsd:documentation>SAonly</xsd:documentation>
                        </xsd:annotation>
                      </xsd:enumeration>
                      <xsd:enumeration value="generic">
                        <xsd:annotation>
                          <xsd:documentation>generic</xsd:documentation>
                        </xsd:annotation>
                      </xsd:enumeration>
                    </xsd:restriction>
                  </xsd:simpleType>
                </xsd:attribute>
              </xsd:extension>
            </xsd:simpleContent>
          </xsd:complexType>
        </xsd:element>
        <xsd:element minOccurs="1" maxOccurs="1" name="Sender">
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:enumeration value="Individual">
                <xsd:annotation>
                  <xsd:documentation>Individual</xsd:documentation>
                </xsd:annotation>
              </xsd:enumeration>
              <xsd:enumeration value="Company">
                <xsd:annotation>
                  <xsd:documentation>Company</xsd:documentation>
                </xsd:annotation>
              </xsd:enumeration>
              <xsd:enumeration value="Agent">
                <xsd:annotation>
                  <xsd:documentation>Agent</xsd:documentation>
                </xsd:annotation>
              </xsd:enumeration>
              <xsd:enumeration value="Bureau">
                <xsd:annotation>
                  <xsd:documentation>Bureau</xsd:documentation>
                </xsd:annotation>
              </xsd:enumeration>
              <xsd:enumeration value="Partnership">
                <xsd:annotation>
                  <xsd:documentation>Partnership</xsd:documentation>
                </xsd:annotation>
              </xsd:enumeration>
              <xsd:enumeration value="Trust">
                <xsd:annotation>
                  <xsd:documentation>Trust</xsd:documentation>
                </xsd:annotation>
              </xsd:enumeration>
              <xsd:enumeration value="Employer">
                <xsd:annotation>
                  <xsd:documentation>Employer</xsd:documentation>
                </xsd:annotation>
              </xsd:enumeration>
              <xsd:enumeration value="Government">
                <xsd:annotation>
                  <xsd:documentation>Government</xsd:documentation>
                </xsd:annotation>
              </xsd:enumeration>
              <xsd:enumeration value="Acting in Capacity">
                <xsd:annotation>
                  <xsd:documentation>Acting in Capacity</xsd:documentation>
                </xsd:annotation>
              </xsd:enumeration>
              <xsd:enumeration value="Other">
                <xsd:annotation>
                  <xsd:documentation>Other</xsd:documentation>
                </xsd:annotation>
              </xsd:enumeration>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Contains">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element minOccurs="1" maxOccurs="unbounded" name="Reference">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element minOccurs="1" maxOccurs="1" name="Namespace" type="xsd:anyURI"/>
              <xsd:element minOccurs="1" maxOccurs="1" name="SchemaVersion">
                <xsd:simpleType>
                  <xsd:restriction base="ct:CT_IRheaderStringType">
                    <xsd:maxLength value="13"/>
                    <xsd:pattern value="[0-9]{4}\-v[0-9]{1,3}\.[0-9]{1,3}(\.[0-9]{1,3})?"/>
                  </xsd:restriction>
                </xsd:simpleType>
              </xsd:element>
              <xsd:element minOccurs="1" maxOccurs="1" name="TopElementName" type="xsd:NCName"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="AttachmentStructure">
    <xsd:simpleContent>
      <xsd:extension base="xsd:base64Binary">
        <xsd:attribute use="required" name="Type">
          <xsd:simpleType>
            <xsd:restriction base="ct:CT_CTstringType">
              <xsd:enumeration value="other">
                <xsd:annotation>
                  <xsd:documentation>Other type of attachment</xsd:documentation>
                </xsd:annotation>
              </xsd:enumeration>
              <xsd:enumeration value="amendment other">
                <xsd:annotation>
                  <xsd:documentation>Amendment other</xsd:documentation>
                </xsd:annotation>
              </xsd:enumeration>
              <xsd:enumeration value="computations">
                <xsd:annotation>
                  <xsd:documentation>Computations and schedules as required</xsd:documentation>
                </xsd:annotation>
              </xsd:enumeration>
              <xsd:enumeration value="accounts">
                <xsd:annotation>
                  <xsd:documentation>Required company accounts</xsd:documentation>
                </xsd:annotation>
              </xsd:enumeration>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute use="required" name="Format">
          <xsd:simpleType>
            <xsd:restriction base="ct:CT_CTstringType">
              <xsd:enumeration value="pdf">
                <xsd:annotation>
                  <xsd:documentation>Portable Document Format</xsd:documentation>
                </xsd:annotation>
              </xsd:enumeration>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute use="required" name="Filename" type="ct:CTfilenameType"/>
        <xsd:attribute use="optional" name="Size" type="xsd:integer"/>
        <xsd:attribute use="optional" name="Description" type="ct:CTexcludedCharsStringType"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="CompanyReference">
    <xsd:sequence>
      <xsd:element minOccurs="1" maxOccurs="1" name="DistrictNumber">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:length value="3"/>
            <xsd:pattern value="[0-9]{3}"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="1" maxOccurs="1" name="UTR" type="ct:CT_UTRtype"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="PeriodType">
    <xsd:sequence>
      <xsd:element minOccurs="1" maxOccurs="1" name="From" type="xsd:date"/>
      <xsd:element minOccurs="1" maxOccurs="1" name="To" type="xsd:date"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="IRnonNegativeWholeUnitsMonetaryStructure">
    <xsd:simpleContent>
      <xsd:extension base="ct:CT_IRnonNegativeWholeUnitsMonetaryType">
        <xsd:attribute use="optional" name="Currency" type="ct:CT_ISOcurrencyType"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="CTwholePoundStructure">
    <xsd:simpleContent>
      <xsd:restriction base="ct:IRnonNegativeWholeUnitsMonetaryStructure">
        <xsd:maxInclusive value="99999999999.00"/>
      </xsd:restriction>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="CTpoundPenceStructure">
    <xsd:simpleContent>
      <xsd:restriction base="ct:CT_IRnonNegativeMonetaryStructure">
        <xsd:maxInclusive value="99999999999.99"/>
      </xsd:restriction>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="CTaddressStructure">
    <xsd:sequence>
      <xsd:element minOccurs="2" maxOccurs="3" name="Line">
        <xsd:simpleType>
          <xsd:restriction base="ct:CT_CTaddressLineType">
            <xsd:maxLength value="28"/>
            <xsd:pattern value="[^!£$%\^\*\+€=_\{\}\[\];:@~#?&lt;&gt;|\\]*"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="1" name="AdditionalLine">
        <xsd:simpleType>
          <xsd:restriction base="ct:CT_CTaddressLineType">
            <xsd:maxLength value="18"/>
            <xsd:pattern value="[^!£$%\^\*\+€=_\{\}\[\];:@~#?&lt;&gt;|\\]*"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="1" name="PostCode">
        <xsd:simpleType>
          <xsd:restriction base="ct:CT_PostCodeType">
            <xsd:maxLength value="8"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="XBRLsubmissionDetails">
    <xsd:sequence>
      <xsd:element minOccurs="1" maxOccurs="1" name="Instance">
        <xsd:complexType>
          <xsd:choice>
            <xsd:element minOccurs="1" maxOccurs="unbounded" name="EncodedInlineXBRLDocument">
              <xsd:complexType>
                <xsd:simpleContent>
                  <xsd:extension base="xsd:base64Binary">
                    <xsd:attribute use="optional" name="Filename" type="ct:CTfilenameType"/>
                    <xsd:attribute use="optional" name="entryPoint" type="ct:CT_YesType"/>
                  </xsd:extension>
                </xsd:simpleContent>
              </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="1" maxOccurs="1" name="RawXBRLDocument">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:any namespace="http://www.xbrl.org/2003/instance" processContents="skip" minOccurs="1" maxOccurs="1"/>
                </xsd:sequence>
                <xsd:attribute use="optional" name="Filename" type="ct:CTfilenameType"/>
                <xsd:anyAttribute namespace="##other" processContents="skip"/>
              </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="1" maxOccurs="unbounded" name="InlineXBRLDocument">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:any namespace="http://www.w3.org/1999/xhtml" processContents="skip" minOccurs="1" maxOccurs="1"/>
                </xsd:sequence>
                <xsd:attribute use="optional" name="Filename" type="ct:CTfilenameType"/>
                <xsd:attribute use="optional" name="entryPoint" type="ct:CT_YesType"/>
              </xsd:complexType>
            </xsd:element>
          </xsd:choice>
          <xsd:attribute use="optional" name="Filename" type="ct:CTfilenameType"/>
          <xsd:attribute use="optional" name="BaseTaxonomyVersionDate" type="ct:CT_DateType"/>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="1" name="Extension">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="1" maxOccurs="1" name="Taxonomy">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:any namespace="http://www.w3.org/2001/XMLSchema" processContents="skip" minOccurs="1" maxOccurs="1"/>
                </xsd:sequence>
                <xsd:attribute use="required" name="Filename" type="ct:CTfilenameType"/>
              </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="5" name="Linkbase">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:any namespace="http://www.xbrl.org/2003/linkbase" processContents="skip" minOccurs="1" maxOccurs="1"/>
                </xsd:sequence>
                <xsd:attribute use="required" name="Type">
                  <xsd:simpleType>
                    <xsd:restriction base="ct:CT_CTstringType">
                      <xsd:enumeration value="label">
                        <xsd:annotation>
                          <xsd:documentation>label linkbase</xsd:documentation>
                        </xsd:annotation>
                      </xsd:enumeration>
                      <xsd:enumeration value="definition">
                        <xsd:annotation>
                          <xsd:documentation>definition linkbase</xsd:documentation>
                        </xsd:annotation>
                      </xsd:enumeration>
                      <xsd:enumeration value="presentation">
                        <xsd:annotation>
                          <xsd:documentation>presentation linkbase</xsd:documentation>
                        </xsd:annotation>
                      </xsd:enumeration>
                      <xsd:enumeration value="calculation">
                        <xsd:annotation>
                          <xsd:documentation>calculation linkbase</xsd:documentation>
                        </xsd:annotation>
                      </xsd:enumeration>
                      <xsd:enumeration value="reference">
                        <xsd:annotation>
                          <xsd:documentation>reference linkbase</xsd:documentation>
                        </xsd:annotation>
                      </xsd:enumeration>
                    </xsd:restriction>
                  </xsd:simpleType>
                </xsd:attribute>
                <xsd:attribute use="required" name="Filename" type="ct:CTfilenameType"/>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="XBRLsubmission">
    <xsd:choice minOccurs="1" maxOccurs="1">
      <xsd:element minOccurs="1" maxOccurs="1" name="Accounts" type="ct:XBRLsubmissionDetails"/>
      <xsd:sequence>
        <xsd:element minOccurs="1" maxOccurs="1" name="Computation" type="ct:XBRLsubmissionDetails"/>
        <xsd:element minOccurs="0" maxOccurs="1" name="Accounts" type="ct:XBRLsubmissionDetails"/>
      </xsd:sequence>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="ChargeAndAllowance">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="1" name="BalancingCharges" type="ct:CTwholePoundStructure"/>
      <xsd:element minOccurs="0" maxOccurs="1" name="CapitalAllowances" type="ct:CTwholePoundStructure"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ArisingAndMaximum">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="1" name="Arising" type="ct:CTwholePoundStructure"/>
      <xsd:element minOccurs="0" maxOccurs="1" name="SurrenderMaximum" type="ct:CTwholePoundStructure"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="CTexcludedCharsStringType">
    <xsd:restriction base="ct:CT_CTstringType">
      <xsd:pattern value="[^£$#~€]+"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="CTfilenameType">
    <xsd:restriction base="ct:CT_CTstringType">
      <xsd:pattern value="[^£$#~|€/\\:*&quot;&lt;&gt;]+"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="CTYesNoNAType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="no">
        <xsd:annotation>
          <xsd:documentation>No</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="yes">
        <xsd:annotation>
          <xsd:documentation>Yes</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="na">
        <xsd:annotation>
          <xsd:documentation>Not applicable</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="CTtenDigitInt">
    <xsd:restriction base="xsd:nonNegativeInteger">
      <xsd:maxInclusive value="9999999999"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="CTalphaNumericStringType">
    <xsd:restriction base="ct:CT_CTstringType">
      <xsd:pattern value="[A-Za-z0-9]*"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="CTA">
    <xsd:sequence>
      <xsd:element minOccurs="1" maxOccurs="1" name="BeforeEndPeriod" type="ct:YesNoType"/>
      <xsd:element minOccurs="0" maxOccurs="1" name="LoansInformation">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="1" maxOccurs="unbounded" name="Loan">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element minOccurs="1" maxOccurs="1" name="Name">
                    <xsd:simpleType>
                      <xsd:restriction base="ct:CTexcludedCharsStringType">
                        <xsd:minLength value="2"/>
                        <xsd:maxLength value="56"/>
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                  <xsd:element minOccurs="1" maxOccurs="1" name="AmountOfLoan" type="ct:CT_CTnonZeroWholePoundStructure"/>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="1" maxOccurs="1" name="TotalLoans">
              <xsd:complexType>
                <xsd:simpleContent>
                  <xsd:restriction base="ct:CT_CTnonZeroWholePoundStructure">
                    <xsd:maxInclusive value="99999999999.00"/>
                  </xsd:restriction>
                </xsd:simpleContent>
              </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="1" maxOccurs="1" name="TaxChargeable" type="ct:CT_CTnonZeroPoundPenceStructure"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="1" name="ReliefEarlierThan">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="1" maxOccurs="unbounded" name="Loan">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element minOccurs="1" maxOccurs="1" name="Name">
                    <xsd:simpleType>
                      <xsd:restriction base="ct:CTexcludedCharsStringType">
                        <xsd:minLength value="2"/>
                        <xsd:maxLength value="56"/>
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                  <xsd:element minOccurs="0" maxOccurs="1" name="AmountRepaid" type="ct:CT_CTnonZeroWholePoundStructure"/>
                  <xsd:element minOccurs="0" maxOccurs="1" name="AmountReleasedOrWrittenOff" type="ct:CT_CTnonZeroWholePoundStructure"/>
                  <xsd:element minOccurs="1" maxOccurs="1" name="Date" type="ct:CT_DateType"/>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" name="TotalAmountRepaid">
              <xsd:complexType>
                <xsd:simpleContent>
                  <xsd:restriction base="ct:CT_CTnonZeroWholePoundStructure">
                    <xsd:maxInclusive value="99999999999.00"/>
                  </xsd:restriction>
                </xsd:simpleContent>
